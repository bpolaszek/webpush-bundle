        "symfony/routing": "^5.4 || ^6.0 || ^7.0",
        "symfony/security-core": "^5.4 || ^6.0 || ^7.0",
        "symfony/var-dumper": "^5.4 || ^6.0 || ^7.0",
        "symfony/yaml": "^5.4 || ^6.0 || ^7.0",
        "twig/twig": "^2.0 || ^3.0"
    },
    "autoload": {
        "psr-4": {
            "BenTools\\WebPushBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BenTools\\WebPushBundle\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
